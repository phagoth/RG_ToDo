new_project = $("<%= escape_javascript(render @project) %>").appendTo("#project-container");
new_project.hover(
    function () {
      $(this).find(".small-buttons").removeClass("hide");
    },
    function () {
      $(this).find(".small-buttons").addClass("hide");
    }
  );

new_project.find(".input_title").hide();
new_project.find(".edit-project").on('click', function(){
    $(this).parents('div').find('.label_title').toggle();
    $(this).parents('div').find('.input_title').toggle();
    
    return false;
  });